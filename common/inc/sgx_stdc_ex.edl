            propagate_errno;
 
        ssize_t u_writev(
            int fd,
            [in, count=iovcnt] struct _iovec_t* iov,
            int iovcnt)
            propagate_errno;

        int u_access(
            [in, string] const char* pathname,
            int mode)
            propagate_errno;

