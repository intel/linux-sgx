            unsigned int count)
            propagate_errno;

        int u_fcntl(
            int fd, 
            int cmd, 
            int arg, 
            uint64_t argsize, 
            [in,out,size=argsize] void* argout)
            propagate_errno;

        int u_open(
            [in, string] const char* pathname, 
