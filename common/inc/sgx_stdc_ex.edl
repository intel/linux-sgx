enclave {

    include "dirent.h"
    include "stdio.h"
    include "time.h"
    include "fcntl.h"
    include "pwd.h"
    include "grp.h"
    include "netdb.h"
    include "sys/time.h"
    include "sys/stat.h"
    include "sys/select.h"
    include "sys/socket.h"
    include "sys/poll.h"
    include "sys/utsname.h"
    include "sys/epoll.h"
    include "sys/types.h"

    struct _iovec_t
    {
        [size=iov_len] void* iov_base;
        size_t iov_len;
    };

    untrusted {
        DIR *u_opendir(
            [in, string] const char* name)
            propagate_errno;

        struct dirent *u_readdir(
            [user_check] DIR *dirp)
